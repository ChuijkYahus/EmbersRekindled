{
  "multipart": [
    {
      "apply": {
        "model": "embersrekindled:block/ember_ejector"
      },
      "when": {
        "facing": "up"
      }
    },
    {
      "apply": {
        "model": "embersrekindled:block/ember_ejector",
        "x": 180
      },
      "when": {
        "facing": "down"
      }
    },
    {
      "apply": {
        "model": "embersrekindled:block/ember_ejector",
        "x": 90
      },
      "when": {
        "facing": "north"
      }
    },
    {
      "apply": {
        "model": "embersrekindled:block/ember_ejector",
        "x": 90,
        "y": 180
      },
      "when": {
        "facing": "south"
      }
    },
    {
      "apply": {
        "model": "embersrekindled:block/ember_ejector",
        "x": 90,
        "y": 90
      },
      "when": {
        "facing": "east"
      }
    },
    {
      "apply": {
        "model": "embersrekindled:block/ember_ejector",
        "x": 90,
        "y": 270
      },
      "when": {
        "facing": "west"
      }
    },
    {
      "apply": {
        "model": "embersrekindled:block/fluid_pipe_end"
      },
      "when": {
        "AND": [
          {
            "OR": [
              {
                "facing": "east|west",
                "front": "true"
              },
              {
                "facing": "south|north",
                "front": "true"
              }
            ]
          }
        ]
      }
    },
    {
      "apply": {
        "model": "embersrekindled:block/fluid_pipe_end_2",
        "x": 180
      },
      "when": {
        "AND": [
          {
            "OR": [
              {
                "facing": "east|west",
                "right": "true"
              },
              {
                "facing": "south|north",
                "right": "true"
              }
            ]
          }
        ]
      }
    },
    {
      "apply": {
        "model": "embersrekindled:block/fluid_pipe_end",
        "x": 90,
        "y": 180
      },
      "when": {
        "AND": [
          {
            "OR": [
              {
                "back": "true",
                "facing": "east|west"
              },
              {
                "facing": "up|down",
                "front": "true"
              }
            ]
          }
        ]
      }
    },
    {
      "apply": {
        "model": "embersrekindled:block/fluid_pipe_end_2",
        "x": 90
      },
      "when": {
        "AND": [
          {
            "OR": [
              {
                "facing": "east|west",
                "left": "true"
              },
              {
                "facing": "up|down",
                "right": "true"
              }
            ]
          }
        ]
      }
    },
    {
      "apply": {
        "model": "embersrekindled:block/fluid_pipe_end",
        "x": 90,
        "y": 90
      },
      "when": {
        "AND": [
          {
            "OR": [
              {
                "back": "true",
                "facing": "up|down"
              },
              {
                "back": "true",
                "facing": "south|north"
              }
            ]
          }
        ]
      }
    },
    {
      "apply": {
        "model": "embersrekindled:block/fluid_pipe_end_2",
        "x": 90,
        "y": 270
      },
      "when": {
        "AND": [
          {
            "OR": [
              {
                "facing": "up|down",
                "left": "true"
              },
              {
                "facing": "south|north",
                "left": "true"
              }
            ]
          }
        ]
      }
    }
  ]
}